{
	"runner": {
		"executablePath": "dot",
		"outputFormat": "svg",
		"fileName": "out",
		"outputDir": "output"
	},
	"model": {
		"classes": [
			"javax.swing.JFrame",
			"java.awt.Frame"
		],
		"blackList":[
			"java",
			"sun"
		],
		"isRecursive": true
	},
	"graphviz": {
		"nodeSep": "1.0",
		"rankDir": "BT"
	},
	"classParser": {
		"modifierFilter": "public"
	},
	"dependencyInversion": {
		"color": "yellow",
		"whiteList": [
			"java.lang",
			"java.util"
		]
	},
	"engine": {
		"analyzer_key":[
			"analyzer.classParser.ClassParserAnalyzer",
			"analyzer.relationParser.RelationParserAnalyzer",
			"analyzer.classParser.ClassParserAnalyzer",
			"analyzer.dependencyInversion.DependencyInversionAnalyzer"
		],
		"synthetic":true
	}
}
